{
    "doctype": "Report",
    "name": "WHT Certificate Register",
    "module": "Print Designer",
    "report_name": "WHT Certificate Register",
    "report_type": "Script Report",
    "ref_doctype": "Payment Entry",
    "is_standard": "No",
